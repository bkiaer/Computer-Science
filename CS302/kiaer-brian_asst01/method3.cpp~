#include <iostream>
#include <vector>
#include <cstdlib> 
using namespace std;

int main()
{
  vector<int> v;
  int randin1;
  int randin2;
  int temp;
  int size;
  cout <<"Please enter an integer" << endl;
  cin>>size;
  v.resize(size);
  for(int i=0; i < v.size(); i++)
    {
      v[i] = i+1; 
    }
    for(int j = 0; j < v.size(); j++)
    {
      randin1 = rand ()% v.size();
      randin2 = rand ()% v.size (); 
       if(randin1 == randin2) 
	{
	      randin1 = rand ()% v.size();
	      randin2 = rand ()% v.size();
	    
	    }
          if(randin1 != randin2)
	    {
	      temp = v[randin1];
	  	v[randin1] = v[randin2];
		v[randin2] = temp;
	}
	}
  return 0; 
}
